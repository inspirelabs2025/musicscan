{
  "version": 2,
  "routes": [
    {
      "src": "^/sitemap\\.xml$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/functions/v1/sitemap-proxy?file=sitemap-index.xml",
      "headers": { "Cache-Control": "public, max-age=900" }
    },
    {
      "src": "^/sitemaps/(.*)$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/functions/v1/sitemap-proxy?file=$1",
      "headers": { "Cache-Control": "public, max-age=900" }
    },
    {
      "src": "^/sm/(.*)$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/functions/v1/sitemap-proxy?file=$1",
      "headers": { "Cache-Control": "public, max-age=900" }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/lovable-uploads/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
