{
  "version": 2,
  "routes": [
    {
      "src": "^/sitemap\\.xml$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/functions/v1/sitemap-proxy?file=sitemap-index.xml",
      "headers": { "Cache-Control": "public, max-age=900" }
    },
    {
      "src": "^/sitemaps/(.*)$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/functions/v1/sitemap-proxy?file=$1",
      "headers": { "Cache-Control": "public, max-age=900" }
    },
    {
      "src": "^/sm/(.*)$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/functions/v1/sitemap-proxy?file=$1",
      "headers": { "Cache-Control": "public, max-age=900" }
    },
    {
      "src": "^/lovable-uploads/(.*)$",
      "dest": "https://ssxbpyqnjfiyubsuonar.supabase.co/storage/v1/object/public/vinyl_images/$1",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
